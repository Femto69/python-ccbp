n=int(input())
for i in range(1,n+1):
    str1=""
    s1=1
    str2=""
    s2=1
    if(i==1 or i==2 or i==n):
        for j in range(i):
            str1=str1+str(j+1)+" "
            s1=s1+1
        print(str1)
    else:
        for k in range(i):
            if(k==0 or k==(i-1)):
                str2=str2+str(k+1)+" "
                s2=s2+1
            else:
                str2=str2+" "+" "
        print(str2)
